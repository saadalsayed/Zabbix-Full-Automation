################ devops test ###############################
curr_trans_accepted_devops_test.Query=SELECT count(*) FROM svista.curr_trans c , (select count(REVERSAL) as REVERSAL , UTRNNO FROM svista.CURR_TRANS CURR_TRANS where \ CURR_TRANS.udate=TO_NUMBER(TO_CHAR(sysdate, 'YYYYMMDD')) AND RESP = -1 group by UTRNNO) reversal WHERE udate=TO_NUMBER(TO_CHAR(sysdate, 'YYYYMMDD')) AND TO_CHAR((c.udate) || LPAD(c.time, 6, 0)) BETWEEN TO_CHAR(sysdate - 1/(24*60),'YYYYMMDDHH24MISS') AND TO_CHAR(sysdate, 'YYYYMMDDHH24MISS') AND RESP = -1 AND reversal.REVERSAL = 1 AND reversal.utrnno=c.utrnno AND c.UDATE=TO_NUMBER(TO_CHAR(sysdate, 'YYYYMMDD')) AND sv_core.Get_string_data(sv_core.Get_tag_data_f(205,bpc_addldata)) = 4512
curr_trans_refused_devops_test.Query=select count(distinct(ct.UTRNNO)) from SVISTA.CURR_TRANS ct where (ct.reversal=1 or ct.resp !=-1) and ct.udate = TO_NUMBER (TO_CHAR (SYSDATE, 'YYYYMMDD')) AND TO_CHAR ((ct.udate) || LPAD (ct.time, 6, 0)) BETWEEN TO_CHAR (SYSDATE- 1/ ( 24* 60),'YYYYMMDDHH24MISS') AND TO_CHAR (SYSDATE,'YYYYMMDDHH24MISS') and sv_core.Get_string_data(sv_core.Get_tag_data_f(205,bpc_addldata)) = 4512
